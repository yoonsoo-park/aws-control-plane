{
	"author": "nCino",
	"bin": {
		"app": "dist/bin/app.js"
	},
	"dependencies": {
		"@jsforce/jsforce-node": "^3.4.1",
		"@ncino/aws-cdk": "^3.58.0",
		"@ncino/aws-sdk": "^3.12.0",
		"@ncino/devops-deploy-infrastructure": "^2.7.0",
		"@ncino/lib-salesforce": "^1.3.0",
		"@types/node": "^22.1.0",
		"aws-lambda": "^1.0.7",
		"aws-sdk": "^2.1613.0",
		"datadog-lambda-js": "^8.108.0",
		"jsforce": "^3.4.1"
	},
	"description": "OmniChannel Control Plane for centralized nCino product access",
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^10.1.3",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.1",
		"@types/aws-lambda": "^8.10.137",
		"@types/jest": "^29.5.12",
		"@types/jsforce": "^1.11.5",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"esbuild": "^0.23.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-promise": "^7.1.0",
		"eslint-plugin-unused-imports": "^3.2.0",
		"jest": "^29.7.0",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.4",
		"ts-node": "^10.9.2",
		"typescript": "~5.5.4"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=20.4.0",
		"npm": ">=9.5.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yoonsoo-park/aws-control-plane"
	},
	"scripts": {
		"aws:deploy": "cdk deploy --all -c suffix=$(whoami) -c stage=blue -c devMode=true --require-approval never",
		"aws:destroy": "cdk destroy OmniChannelControlPlane-WebsiteStack-$(whoami) OmniChannelControlPlane-ApiStack-$(whoami) -c suffix=$(whoami) --force",
		"build": "tsc",
		"format": "eslint . --fix --ext .js,.ts --quiet",
		"prebuild": "npm run format",
		"pretest": "npm run build",
		"semantic-release": "npx semantic-release",
		"test": "jest --passWithNoTests",
		"watch": "tsc -w",
		"web": "cd src/web && npm run dev",
		"build:web": "cd src/web && npm run build",
		"test:web": "cd src/web && npm run test"
	},
	"types": "lib/index.d.ts",
	"version": "1.0.0"
}
